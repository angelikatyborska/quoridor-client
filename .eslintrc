{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "extends": "airbnb",
  "rules": {
    "react/prop-types": 1,

    "brace-style": [1, "stroustrup", {"allowSingleLine": true}]
  }
}